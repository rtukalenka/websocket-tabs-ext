{
  "manifest_version": 3,
  "name": "Websocket tabs extension",
  "description": "Websocket with shared service worker websocket",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://*.github.com/*"
      ]
    }
  ]
}